{
    "targets": {
        "opnosis": {
            "outFile": "build/Opnosis.wasm",
            "use": ["abort=src/opnosis/index/abort"]
        },
        "orange-coin": {
            "outFile": "build/OrangeCoin.wasm",
            "use": ["abort=src/orange-coin/index/abort"]
        }
    },
    "options": {
        "sourceMap": false,
        "optimizeLevel": 3,
        "shrinkLevel": 1,
        "converge": true,
        "noAssert": false,
        "enable": [
            "sign-extension",
            "mutable-globals",
            "nontrapping-f2i",
            "bulk-memory",
            "simd",
            "reference-types",
            "multi-value"
        ],
        "runtime": "stub",
        "memoryBase": 0,
        "initialMemory": 1,
        "exportStart": "start",
        "transform": "@btc-vision/opnet-transform"
    }
}
